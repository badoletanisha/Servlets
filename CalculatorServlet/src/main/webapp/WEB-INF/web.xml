<servlet-name>division</servlet-name>
<url-pattern>/divNumbers</url-pattern>
</servlet-mapping>

<servlet>
<servlet-name>modulus</servlet-name>
<servlet-class>com.jsp.Calculator</servlet-class>
</servlet>
